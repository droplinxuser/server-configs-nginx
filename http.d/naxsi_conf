include /etc/nginx/naxsi_rules/naxsi_core.rules;
include /etc/nginx/naxsi_rules/blocking/*.rules;
include /etc/nginx/naxsi_rules/whitelists/*.rules;

SecRulesEnabled;
LearningMode;
LibInjectionSql; 
LibInjectionXss; 

DeniedUrl "/503";
CheckRule "$SQL >= 8" BLOCK;
CheckRule "$RFI >= 8" BLOCK;
CheckRule "$TRAVERSAL >= 4" BLOCK;
CheckRule "$EVADE >= 4" BLOCK;
CheckRule "$XSS >= 8" BLOCK;
